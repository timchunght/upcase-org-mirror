<section class="topic-resources">
  <% if @trail %>
    <span class="divider">
      <h4 class="text">Trail</h4>
    </span>
    <section class="trails-progress">
      <%= render "practice/trail", trail: @trail %>
    </section>
  <% end %>

  <span class="divider">
    <h4 class="text">The Weekly Iteration</h4>
  </span>
  <% @resources["Video"].each do |resource| %>
    <%= render partial: "products/#{resource.to_partial_path}",
      object: resource %>
  <% end %>

  <span class="divider">
    <h4 class="text">Video Tutorials</h4>
  </span>
  <% @resources["VideoTutorial"].each do |resource| %>
    <%= render partial: "products/#{resource.to_partial_path}",
      object: resource %>
  <% end %>
</section>
